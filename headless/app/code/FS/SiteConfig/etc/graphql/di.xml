    <arguments>
      <argument name="extendedConfigData" xsi:type="array">
        <item name="show_banner" xsi:type="string">fsconfig/settings/show_banner</item>
        <item name="show_product_category" xsi:type="string">fsconfig/settings/show_product_category</item> 
        <item name="show_about_us" xsi:type="string">fsconfig/settings/show_about_us</item>     

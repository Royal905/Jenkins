        <item name="show_testimonals" xsi:type="string">fsconfig/settings/show_testimonals</item>
        <item name="home_categories" xsi:type="string">fsconfig/settings/home_categories</item>
        <item name="support_email" xsi:type="string">fsconfig/settings/support_email</item>
        <item name="support_phone" xsi:type="string">fsconfig/settings/support_phone</item>     
      </argument>
    </arguments>
  </type>
</config>
